{
  "manifest_version": 3,
  "name": "Practice Fusion Assistant",
  "version": "1.0",
  "description": "Enhances Practice Fusion EHR with AI-powered suggestions",
  "permissions": ["activeTab"],
  "host_permissions": ["https://*.practicefusion.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.practicefusion.com/*"],
      "js": ["overlay.js", "dataExtractor.js", "contentScript.js"]
    }
  ],
  "action": {
    "default_title": "Toggle Practice Fusion Assistant"
  }
}
